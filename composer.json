{
    "name"             : "primate/framework",
    "description"      : "Primate framework",
    "type"             : "library",
    "keywords"         : [
        "primate",
        "framework",
        "mvc-framework",
        "mvc-pattern",
        "mvc-architecture",
        "mvc"
    ],
    "license"          : "MIT",
    "authors"          : [
        {
            "name"    : "Jagepard",
            "email"   : "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role"    : "Developer"
        }
    ],
    "support"          : {
        "email": "jagepard@yandex.ru",
        "skype": "jagepard.ru"
    },
    "autoload"         : {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require"          : {
        "php" : ">=8.3"
    },
    "require-dev"      : {
        "primate/router"    : "dev-master",
        "primate/container" : "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S 127.0.0.1:8000 -t public"
        ]
    },
    "config"           : {
        "optimize-autoloader": true,
        "prefer-stable"      : true
    }
}
